{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "github": {
    "silent": true
  },
  "redirects": [
    {
      "source": "/((?!coming-soon).*)",
      "destination": "/coming-soon",
      "permanent": false
    }
  ]
} 