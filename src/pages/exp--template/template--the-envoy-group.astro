    /* // -- Helpers -- // */
    /* $section-gutter: 9.375vw; // 5vw
    $section-gutter-large: 9.375vw; */

    .cta {
      color: inherit;
      display: inline-block;
      font-family: var(--font-family-body);
      font-size: 14px;
      letter-spacing: 0;
      line-height: (28/18);
      position: relative;
      text-decoration: none;
      vertical-align: top;

      @media (min-width: 1400px) {
        font-size: 18px;
      }

      &:after {
        border-top: 1px solid;
        bottom: 5px;
        content: "";
        left: 50%;
        position: absolute;
        transform: translateX(-50%);
        width: 100%;
      }
    }

    @mixin section-padding(
      $padding-top: 150px,
      $padding-right: $section-gutter,
      $padding-bottom: 150px,
      $padding-left: $section-gutter,
      $padding-top-large: 300px,
      $padding-right-large: $section-gutter-large,
      $padding-bottom-large: 300px,
      $padding-left-large: $section-gutter-large
    ) {
      padding-bottom: $padding-bottom;
      padding-left: $padding-left;
      padding-right: $padding-right;
      padding-top: $padding-top;

      @media (min-width: 1600px) {
        padding-bottom: $padding-bottom-large;
        padding-left: $padding-left-large;
        padding-right: $padding-left-large;
        padding-top: $padding-top-large;
      }
    }

    .section {
      @include section-padding;
    }

    .section--flush-gutters {
      @include flush-gutters;
    }

    .wrap {
      @include wrap;
    }

    @mixin title($font-size-largest: true) {
      color: #333333;
      font-family: var(--font-family-body);
      font-size: 32px;
      font-weight: normal;
      letter-spacing: 0;
      line-height: (70/60);

      @media (min-width: 1400px) {
        font-size: 48px;
      }

      @if ($font-size-largest) {
        @media (min-width: 1700px) {
          font-size: 60px;
        }
      }
    }

    .title {
      @include title;
    }

    /* // -- Nav -- // */

    .nav__link {
      color: inherit;
      display: none;
      font-family: var(--font-family-body);
      font-size: 16px;
      letter-spacing: 0;
      line-height: 20px;
      position: relative;
      text-decoration: none;
      transition: color 150ms ease-in-out;

      &:before {
        background: white;
        bottom: -5px;
        height: 1px;
        left: -15px;
        content: "";
        margin-bottom: -3px;
        position: absolute;
        transform: scaleX(0);
        transition: transform 300ms ease-in-out, height 300ms ease-in-out;
        transition-delay: 300ms, 0ms;
        width: calc(100% + 32px);
        will-change: transform;
        z-index: -1;
      }

      &:hover {
        color: var(--color-blue);
        transition-delay: 400ms;

        &:before {
          height: 36px;
          transform: scaleX(1);
          transition-delay: 0ms, 300ms;
        }
      }

      @media (min-width: 800px) {
        display: inline-block;
      }

      + .nav__link {
        margin-left: 44px;
      }
    }

    .nav-scroll__link:hover {
      color: #151515;
    }

    /* // -- Header -- // */

    .logo {
      position: relative;

      svg {
        display: block;
      }
    }

    .logo__text {
      display: none;
      padding-top: 25px;
      position: absolute;
      right: 0;
      .nav-scroll__link:hover {
        color: #151515;
      }

      /* // -- Header -- // */

      top: 100%;

      @media (min-width: 800px) {
        display: block;
      }
    }

    .nav,
    .header__menu,
    .logo--top {
      animation: 400ms headerLoad forwards 1 ease-in 1250ms;
      opacity: 0;
      transform: translate3d(0, 7px, 0);
    }

    .header-scroll .nav,
    .header-scroll .logo {
      animation: none;
      opacity: 1;
      transform: none;
    }

    @keyframes headerLoad {
      to {
        opacity: 1;
        transform: translate3d(0, 0, 0);
      }
    }

    .header {
      color: #fff;
      padding: 70px 0 54px;
      left: 0;
      position: absolute;
      right: 0;
      top: 0;
      z-index: 1;
    }

    .header--top {
      transform-origin: center top;
      z-index: 3;
      /* // transition: transform 1s cubic-bezier(.48,0,.12,1);  */
      /* // for parallaxing header on scroll a little */
    }

    .header__wrap {
      align-items: center;
      display: flex;
      justify-content: space-between;
      width: 81.25%;
    }

    /* // -- Header Scroll -- // */
    .header-scroll {
      background-color: #151515;
      background-color: transparentize(#151515, 0.05);
      color: #fff;
      padding-bottom: 22px;
      padding-top: 22px;
      pointer-events: none;
      position: fixed;
      transform: translate3d(0, -200%, 0);
      transition: all 450ms ease-in-out;
      will-change: transform;
      z-index: 101;

      @media (max-width: 799px) {
        .menu--active[data-menu-button-type="scroll"] & {
          pointer-events: auto;
          transform: translate3d(0, 0, 0);
        }
      }
    }

