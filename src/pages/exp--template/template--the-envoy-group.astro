      font-weight: normal;
      font-size: 12px;
      letter-spacing: 0;
      line-height: (30/15);
      margin-bottom: 13px;

      @media (min-width: 1400px) {
        font-size: 15px;
      }
    }

    .showcase__headline ~ .showcase__headline {
      margin-top: 33px;
    }

    .showcase__image {
      background-color: #9b9b9b;
      background-position: center;
      background-repeat: no-repeat;
      background-size: cover;
      margin-bottom: 31px;

      &:before {
        content: "";
        display: block;
        padding-top: percentage(340px/400px);

        @media (min-width: 768px) {
          padding-top: percentage(
            800px/570px
          ); 
          /* // start all images out as portrait until they get big enough as squares to look right with copy not getting taller than image */
        }

        @media (min-width: 1400px) {
          padding-top: percentage(570px/570px);
        }
      }

      @media (min-width: 768px) {
        margin-bottom: 0;
      }
    }

    .showcase__image--portrait {
      @media (min-width: 1400px) {
        &:before {
          padding-top: percentage(800px/570px);
        }
      }
    }

    .showcase__info-wrap {
      @media (min-width: 768px) {
        margin-left: auto;
        margin-right: auto;
        min-width: 320px;
        padding-right: 30px;
        padding-top: 8.25%;
      }

      @media (min-width: 1900px) {
        padding-top: 18%;
      }
    }

    .showcase__items {
      color: #bcbcbc;
      font-family: var(--font-family-body);
      font-size: 14px;
      list-style: none;
      letter-spacing: 0;

      @media (min-width: 1400px) {
        font-size: 17px;
      }

      @media (min-width: 1700px) {
        font-size: 24px;
      }
    }

    .showcase__item {
      margin-bottom: 10px;

      @media (min-width: 1500px) {
        margin-bottom: 20px;
      }
    }

    .showcase__list {
      list-style: none;
      font-size: 15px;
      letter-spacing: 0;
      line-height: (28/18);

      @media (min-width: 1400px) {
        font-size: 18px;
      }
    }

    .showcase__list-item {
    }

    .showcase__media {
      flex-shrink: 0;
      position: relative;
    }

    .showcase__title {
      display: block;
      margin-bottom: 18px;

      @media (min-width: 1000px) {
        font-size: 48px;
        margin-bottom: 28px;
      }
    }

    .showcase__video {
      bottom: 0;
      overflow: hidden;
      left: 0;
      position: absolute;
      right: 0;
      top: 0;

      &:before {
        content: "";
        display: block;
        padding-top: percentage(340px/400px);

        @media (min-width: 768px) {
          padding-top: percentage(570px/570px);
        }
      }
    }

    .showcase__video--portrait {
      @media (min-width: 768px) {
        &:before {
          padding-top: percentage(800px/570px);
        }
      }
    }

    .showcase__video video {
      /* // TODO: fix so videos hold proper ratio of square/po */
      height: 100%;
      left: 50%;
      position: absolute;
      top: 50%;
      transform: translate(-50%, -50%);
      width: auto;
    }

    .showcase__wrap {
      @media (min-width: 768px) {
        display: flex;
        justify-content: space-between;
      }
    }

    .showcase:nth-of-type(odd) .showcase__info-wrap {
      @media (min-width: 768px) {
        margin-left: 0;
      }
    }

    .showcase:nth-of-type(even) .showcase__info-wrap {
      @media (min-width: 768px) {
        padding-right: 0;
        width: 65.3%;
      }
    }

    .showcase:nth-of-type(odd) .showcase__wrap {
      flex-direction: row-reverse;
    }

    .showcase--last-light {
      padding-bottom: 250px;

      @media (min-width: 1700px) {
        padding-bottom: 50px;
      }
    }

    /* // -- Showcase Dark -- // */
    .showcase--dark {
      @include flush-gutters;
      @include flush-gutters-padding;
      background-color: #1b1b1b;
      color: #fff;
      display: flex;
      flex-direction: column;
      justify-content: center;
      min-height: 100vh;
      padding-top: 0;

      @media (min-width: 1400px) and (min-height: 1000px) {
      }
    }

    .showcase--dark .showcase__wrap {
      width: 100%;
    }

    .showcase__copy--dark {
      color: inherit;
      margin-bottom: 33px;
    }

    .showcase__title--dark {
      color: inherit;
    }

    /* // -- People -- // */

    .people {
      @include section-padding(
        $padding-top-large: null,
        $padding-bottom: 100px,
        $padding-bottom-large: null,
        $padding-top: 100px
      );

      @media (min-width: 1400px) {
        padding-top: 140px;
      }
    }

    .people__copy {
      color: #bcbcbc;
      display: block;
      font-size: 24px;
      font-weight: normal;
      letter-spacing: 0;
      margin-bottom: 100px;
      position: relative;

      @media (min-width: 900px) {
        font-size: 32px;
      }

      @media (min-width: 1050px) {
        @include grid-cols(9);
      }

      @media (min-width: 1700px) {
        font-size: 48px;
      }
    }

    .people__items-wrap {
      @media (min-width: 1050px) {
        align-items: flex-end;
        display: flex;
        flex-direction: column;
      }
    }

    .people__items {
      list-style-type: none;

      @media (min-width: 500px) {
        display: flex;
        flex-wrap: wrap;
        margin-left: -30px;
      }
    }

    .people__item {
      color: #9b9b9b;
      font-family: var(--font-family-body);
      font-size: 15px;
      letter-spacing: 0;
      line-height: (28/18);
      margin-bottom: 50px;

      @media (min-width: 500px) {
        display: flex;
        flex-wrap: wrap;
        margin-left: 30px;
        width: calc(50% - 30px);
      }

      @media (min-width: 750px) {
        margin-bottom: 76px;
        width: calc(33.333% - 30px);
      }

      @media (min-width: 1050px) {
        width: calc(25% - 30px);

        &:nth-child(1) {
          margin-left: calc(25% + 30px);
        }

        &:nth-child(2n + 4) {
          margin-left: calc(50% + 30px);
        }
      }

      @media (min-width: 1400px) {
        font-size: 18px;
      }
    }

    .people__name {
      color: #333333;
      display: block;
      font-weight: normal;
      margin-bottom: 11px;
      width: 100%;
    }

    .people__title {
      margin-bottom: 30px;

      @media (min-width: 900px) {
        margin-bottom: 158px;
      }
    }

    /* // -- Grid visualizer -- //å */
    .grid-overlay {
      bottom: 0;
      display: flex;
      left: 50%;
      max-width: 1600px;
      pointer-events: none;
      position: fixed;
      top: 0;
      transform: translateX(-50%);
      transform-origin: center top;
      transition: all 400ms ease-in-out;
      width: 81.25%;
      z-index: 100;

      &.grid--off {
        opacity: 0;
      }
    }

    .grid-overlay__col {
      background-color: transparentize(green, 0.94);
      margin-left: 30px;
      transition: all 300ms ease-in-out;
      width: 8.33333%;

      &:first-child {
        margin-left: 0;
      }
    }

    .grid--off .grid-overlay__col {
      transform: translateY(100%);

      &--2,
      &--6,
      &--8,
      &--10,
      &--12 {
        transition-delay: 150ms;
      }

      &--3,
      &--5,
      &--7,
      &--9,
      &--11 {
        transition-delay: 100ms;
      }
    }

    .grid-overlay__toggle {
      background: none;
      box-shadow: none;
      border: 0;
      bottom: 10px;
      cursor: pointer;
      height: 20px;
      left: 10px;
      opacity: 0.1;
      outline: 0;
      position: fixed;
      transition: all 250ms ease-in-out;
      width: 15px;
      z-index: 100;

      &:hover {
        opacity: 0.2;

        .grid-overlay__toggle__line {
          transform: scaleY(1.3);
        }
      }

      &__line {
        background-color: green;
        bottom: 0;
        position: absolute;
        height: 100%;
        transform-origin: left bottom;
        transition: all 250ms ease-in-out;
        width: 3px;

        &--1 {
          left: 0;
        }

        &--2 {
          margin-left: -1px;
          left: 50%;
          transition-delay: 150ms;
        }

        &--3 {
          margin-left: -1px;
          left: 100%;
          transition-delay: 100ms;
        }
      }
    }

    .grid-overlay__toggle.grid---off {
      opacity: 0;
    }

    /* // -- News -- // */

    .news {
      padding-bottom: 260px;
      padding-top: 50px;
    }

    @media (min-width: 1400px) {
      .news {
        padding-bottom: 280px;
      }
    }

    .news__date {
      color: #9b9b9b;
      display: block;
      font-size: 11px;
      letter-spacing: 0;
      line-height: 1.7857142857;
      margin-bottom: 10px;
    }
    @media (min-width: 1600px) {
      .news__date {
        font-size: 14px;
      }
    }

    .news__header {
      background-color: #fff;
      margin-bottom: 75px;
      position: relative;
      z-index: 1;
      height: 0;
    }
    @media (min-width: 900px) {
      .news__header {
        margin-bottom: 95px;
      }
    }
    @media (min-width: 1050px) {
      .news__header {
        display: flex;
        flex-direction: column;
        justify-content: space-between;
        margin-bottom: 0;
        flex-shrink: 0;
        margin-right: 30px;
        width: calc((((100% - (11 * 30px)) / 12) * 4) + (30px * 3));
      }
      .news__header:before {
        background-color: #fff;
        bottom: -80px;
        content: "";
        position: absolute;
        right: -1px;
        top: 0;
        width: 50vw;
        z-index: -1;
      }
    }

    .news__image {
      background-color: #4a4a4a;
      background-position: center;
      background-repeat: no-repeat;
      background-size: cover;
      margin-bottom: 25px;
    }
    @media (min-width: 1600px) {
      .news__image {
        margin-bottom: 42px;
      }
    }
    .news__image:before {
      content: "";
      display: block;
      padding-top: 85%;
    }

    .news__items {
      display: flex;
      list-style: none;
    }

    @keyframes fadeIn {
      to {
        opacity: 1;
      }
    }
    .news__item {
      animation: 300ms fadeIn ease-in-out 1 forwards;
      flex-shrink: 0;
      margin-left: 30px;
      opacity: 0;
      width: 100%;
      /* // width: calc(100% - 30px); */
    }

    @media (min-width: 500px) {
      .news__item {
        margin-left: 30px;
        width: calc(100% / 2 - 15px);
      }
    }

    @media (min-width: 750px) {
      .news__item {
        margin-left: 30px;
        width: calc(100% / 3 - 20px);
      }
    }

    .news__items figure {
      transition: all 0.25s ease;
    }

    .news__items figure.hidden {
      opacity: 0;
    }

    .news__items figure.prev {
      transition: all 0.25s ease;
      transition-delay: 0.25s;
    }

    .news__items figure.prev.hidden {
      transition: none;
      transition-delay: none;
      opacity: 0;
    }

    .news__name {
      font-family: var(--font-family-body);
      font-size: 15px;
      font-weight: normal;
      color: #333333;
      letter-spacing: 0;
      line-height: 1.5;
    }
    @media (min-width: 1600px) {
      .news__name {
        font-size: 24px;
      }
    }

    .news__wrap {
      width: 81.25%;
    }
    @media (min-width: 1050px) {
      .news__wrap {
        display: flex;
      }
    }

    .news-carousel {
      position: relative;
      width: 100%;
    }

    @media (min-width: 1050px) {
      .news-carousel {
        left: calc(100% / 3 + 10px);
      }
    }

    .news-carousel .news-carousel__btn {
      background: none;
      border: 0;
      box-shadow: none;
      cursor: pointer;
      outline: none;
    }

    .news-carousel .news-carousel__nav {
      align-items: center;
      border: 1px solid #bcbcbc;
      bottom: -100px;
      color: #bcbcbc;
      display: flex;
      justify-content: space-around;
      font-size: 11px;
      letter-spacing: 0.24px;
      line-height: 30px;
      min-width: 140px;
      padding: 10px 5px;
      position: absolute;
      text-align: center;
      width: calc((((100% - (11 * 30px)) / 12) * 3) + (30px * 2));
      max-width: calc((((100% - (11 * 30px)) / 12) * 2) + (30px * 1.5));
    }
    .news-carousel .news-carousel__nav svg {
      display: block;
    }
    @media (min-width: 1400px) {
      .news-carousel .news-carousel__nav {
        padding: 10px 25px;
      }
    }
    @media (min-width: 1400px) {
      .news-carousel .news-carousel__counter-wrap {
        margin: 0 9px;
      }
    }
    .news-carousel .news-carousel__counter {
      display: inline-block;
      text-align: center;
    }
    .news-carousel .news-carousel__counter--active {
      color: #333333;
      width: 17px;
    }
    .news-carousel .news-carousel__counter--separator {
      width: 15px;
    }
    .news-carousel .news-carousel__counter--total {
      width: 17px;
    }
    .news-carousel .flickity-button {
      display: none;
    }
    .news-carousel .flickity-viewport {
      width: 100%;
      height: 100%;
    }

    /* // -- Newsletter -- // */
    .newsletter {
      background-color: #3d3d45;
      color: #fff;
      padding: 120px 0 150px;
      position: relative;

      @media (min-width: 1400px) {
        padding: 181px 0 203px;
      }
    }

    .newsletter__form {
      position: relative;

      @media (min-width: 500px) {
        @include grid-cols(12);
      }

      @media (min-width: 750px) {
        @include grid-cols(10);
      }

      @media (min-width: 900px) {
        @include grid-cols(8);
      }

      @media (min-width: 1200px) {
        width: calc((((100% - (11 * 30px)) / 12) * 6) + (30px * 5) + 15px);
      }
    }

    .newsletter__input {
      @include title($font-size-largest: false);
      background-color: transparent;
      border-width: 0 0 1px;
      color: inherit;
      height: 64px;
      outline: none;
      display: block;
      width: 100%;

      @media (min-width: 1400px) {
        height: 80px;
      }
    }

    @keyframes blink {
      0% {
        opacity: 0;
      }

      50% {
        opacity: 1;
      }

      100% {
        opacity: 0;
      }
    }

    .newsletter__input:focus ~ .newsletter__blinker {
      visibility: hidden;
    }

    .newsletter__blinker {
      animation-duration: 1s;
      animation-fill-mode: forwards;
      animation-name: blink;
      animation-iteration-count: infinite;
      background-color: #fff;
      height: 56px;
      left: 0;
      opacity: 0;
      position: absolute;
      top: 14px;
      width: 2px;
    }

    .newsletter__input:focus + label {
      top: -5px;
      transform: scale(0.25);
    }

    .newsletter__label {
      @include title($font-size-largest: false);
      color: inherit;
      left: 15px;
      position: absolute;
      top: 22px;
      transform-origin: left top;
      transition: all 300ms ease-in-out;

      @media (min-width: 1400px) {
        top: 10px;
      }
    }

    .newsletter__wrap {
      width: 81.25%;
    }

    input:-webkit-autofill {
      -webkit-box-shadow: 0 0 0 80px #3d3d45 inset;
      -webkit-text-fill-color: #fff !important;
    }

    .newsletter {
      &:before {
        content: "";
        position: absolute;
        width: 100%;
        height: 0;
        left: 0;
        bottom: 0;
        background: #0830ff;
        transition: 300ms;
      }

      &.success {
        &:before {
          height: 100%;
        }
      }
    }

    .newsletter__form {
      transition: all 300ms ease-in-out;

      .newsletter.success & {
        opacity: 0 !important;
        pointer-events: none !important;
      }
    }

    label.focus {
      top: -5px;
      transform: scale(0.25);
    }

    .newsletter__messages--success {
      font-family: var(--font-family-body);
      font-size: 32px;
      left: 50%;
      margin-left: auto;
      margin-right: auto;
      max-width: 1600px;
      width: 82.5%;
      opacity: 0;
      pointer-events: none;
      position: absolute;
      top: 50%;
      transform: translate3d(0, 40px, 0);
      transform: translate3d(-50%, 40px, 0);
      transition: all 300ms ease-in-out 150ms;

      .footer.in-view .newsletter.success & {
        opacity: 1;
        pointer-events: auto;
        transform: translate3d(-50%, -50%, 0);
      }
    }

    .newsletter__messages--error {
      transition: 300ms;
      opacity: 0;
      visibility: hidden;
      color: #fff;
      position: absolute;
      bottom: -60px;

      &.show {
        opacity: 1;
        visibility: visible;
        bottom: -40px;
      }

      a {
        color: #fff;
        text-decoration: none;
      }
    }

    /* // -- Footer -- // */
    .footer {
      background-color: #000;
      overflow: hidden;
    }

    .footer[data-scroll-item] {
      opacity: 1;
      transform: none;
      transition: none;
    }

    .footer__copyright {
      color: #ffffff;
      flex-basis: 100%;
      flex-shrink: 0;
      font-family: var(--font-family-body);
      font-size: 12px;
      line-height: 20px;
    }

    .footer__email,
    .footer__social-link {
      color: inherit;
      text-decoration: none;
      transition: all 250ms ease-in-out;

      &:focus,
      &:hover {
        opacity: 0.5;
      }
    }

    .footer__email {
      display: inline-block;
      margin-bottom: 33px;
      margin-top: 2px;
    }

    .footer__info {
      @media (min-width: 1400px) {
        margin-right: 30px;
        @include grid-cols(3);
      }
    }

    .footer__intro {
      display: block;
      font-weight: normal;
    }

    .footer__locations {
      margin-bottom: 44px;
    }

    .footer__social-items {
      list-style: none;
      margin-bottom: 44px;
    }

    .footer__wrap {
      background-color: #000;
      color: #fff;
      font-size: 18px;
      line-height: 28px;
      padding: 141px 9.375%;
      position: relative;
      width: 100%;

      @media (min-width: 1400px) {
        display: flex;
        flex-wrap: wrap;
      }

      @media (min-width: 1600px) {
        font-size: 20px;
        line-height: 30px;
        padding-left: 0;
        padding-right: 0;
        width: 82.5%;
      }
    }

    .footer__social-item + .footer__social-item {
      margin-top: 2px;
    }

    .logo--footer {
      position: absolute;
      top: 146px;
      right: 9.375%;
      z-index: 1;

      [fill="#fff"] {
        fill: var(--color-blue);
      }
    }

    .footer__wrap > *,
    .newsletter__form {
      opacity: 0;
      transform: translateY(60px);
      transition: all 600ms ease-out;
      transition: opacity 400ms ease-out, transform 700ms ease-out;

      .footer.in-view & {
        opacity: 1;
        transform: translateY(0);
      }
    }

    /* // -- Image Carousel -- // */
    .image-carousel {
      margin-bottom: 30px;
      opacity: 0;

      @media (min-width: 768px) {
        margin-bottom: 0;
      }

      &.revealed {
        transition: all 0.25s ease;
        opacity: 1;
      }

      &__image {
        width: 100%;
      }

      .flickity-button {
        display: none;
      }

      .flickity-page-dots {
        bottom: -29px;
        display: flex;
        left: 50%;
        justify-content: center;
        margin: 0 auto;
        min-width: 260px;
        transform: translateX(-50%);
        width: calc(100% - 130px);

        @media (min-width: 900px) {
          bottom: -63px; 
          /* // sits 30px below image */
        }

        @media (min-width: 1700px) {
          bottom: -73px;
           /* // sits 40px below image */
        }

        &.in-view {
          @media (min-width: 768px) {
            .dot {
              margin: 0;
            }
          }
        }

        .dot {
          background: none;
          border: 0;
          border-radius: 0;
          box-shadow: none;
          flex-basis: 12.5%;
          height: 60px;
          margin: 0 20px 0 0;
          opacity: 1;
          position: relative;
          transition: all 400ms ease-in-out;

          &:last-child {
            margin: 0;
          }

          @media (min-width: 1700px) {
            height: 40px;
          }

          /* // line */
          &:after {
            background-color: #cdcdcd;
            bottom: 30px;
            content: "";
            height: 4px;
            position: absolute;
            right: 0;
            left: 0;
            transform-origin: center;
            transform: scaleY(0.5);
            transition: transform 250ms ease;
          }

          &:hover:after {
            transform: scaleY(1);
          }

          &.is-selected:after {
            background-color: #333333;
          }
        }
      }

      &:hover {
        .flickity-page-dots {
          .dot {
            margin: 0 20px 0 0;

            &:last-child {
              margin: 0;
            }
          }
        }
      }

      &:before {
        content: " ";
        display: block;
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: calc(100% + 60px);
      }
    }

    /* // -- Scroll Effects -- // */
    [data-scroll-item] {
      opacity: 0;
      transform: translateY(60px);
      transition: all 600ms ease-out;
      transition: opacity 450ms ease-out, transform 800ms ease-out;

      &.in-view {
        opacity: 1;
        transform: translateY(0);
      }
    }

    /* // -- Page background scroll effects -- // */
    .bg__item {
      background-position: center !important;
      background-repeat: no-repeat !important;
      background-size: cover !important;
      bottom: 0;
      left: 0;
      opacity: 0;
      pointer-events: none;
      position: fixed;
      right: 0;
      top: 0;
      transition: all 450ms ease-in-out;
