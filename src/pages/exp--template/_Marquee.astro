<style>
  /* Marquee Animation Keyframes */
  @keyframes mobileScroll {
    0% {
      transform: translateX(0);
    }
    100% {
      transform: translateX(calc(-50% - 1rem));
    }
  }

  /* Logo Strip Container */
  .logostrip-c {
    justify-content: flex-start;
    align-items: center;
    display: flex;
    overflow: hidden;
    white-space: nowrap;
    -webkit-mask-image: linear-gradient(
      90deg,
      transparent 0%,
      #fff 15%,
      #fff 85%,
      transparent 100%
    );
    mask-image: linear-gradient(
      90deg,
      transparent 0%,
      #fff 15%,
      #fff 85%,
      transparent 100%
    );
    width: 100%;
    position: relative;
  }

  /* Wrapper for smooth animation */
  .logostrip-wrapper {
    display: flex;
    align-items: center;
    width: fit-content;
    will-change: transform;
  }

  /* Logo Strip Collection */
  .logostrip-collection {
    display: flex;
    flex-shrink: 0;
    align-items: center;
  }

  /* Logo Strip List */
  .logostrip-list-c {
    display: flex;
    align-items: center;
    gap: 3rem;
    flex-shrink: 0;
    padding-right: 3rem;
  }

  /* Animation for Strip */
  .animate-strip {
    animation: mobileScroll 25s linear infinite;
  }

  /* Logo Container */
  .hp-hero-logo-w {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-shrink: 0;
    min-width: 120px;
    height: 60px;
  }

  /* Logo Image */
  .hp-hero-logo {
    height: 2.5rem;
    width: auto;
    max-width: 100px;
    object-fit: contain;
    filter: grayscale(100%) opacity(0.6);
    transition: filter 0.3s ease;
  }

  .hp-hero-logo:hover {
    filter: grayscale(0%) opacity(1);
  }

  /* Responsive adjustments */
  @media (max-width: 768px) {
    .animate-strip {
      animation: mobileScroll 15s linear infinite;
    }

    .hp-hero-logo {
      height: 2rem;
      max-width: 80px;
    }

    .logostrip-list-c {
      gap: 2rem;
      padding-right: 2rem;
    }

    .hp-hero-logo-w {
      min-width: 100px;
      height: 50px;
    }
  }

  @media (max-width: 480px) {
    .hp-hero-logo {
      height: 1.5rem;
      max-width: 60px;
    }

    .logostrip-list-c {
      gap: 1.5rem;
      padding-right: 1.5rem;
    }

    .hp-hero-logo-w {
      min-width: 80px;
      height: 40px;
    }
  }
</style>
